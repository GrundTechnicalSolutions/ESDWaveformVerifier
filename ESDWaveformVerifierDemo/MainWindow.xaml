        <TabItem Header="CDM JS-002">
            <local:CDMView DataContext="{Binding Path=CDMVM}" />
        </TabItem>
        <TabItem Header="HBM JS-001">
            <local:HBMView DataContext="{Binding Path=HBMVM}" />
        </TabItem>
    </TabControl>
</Window>
